<div id="<%= dom_id project %>" class="project">
  <p>
    <strong>Title:</strong>
    <%= project.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= project.description %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= project.status %>
  </p>

  <p>
    <strong>Assigned to:</strong>
    <%= project.user.email %>
  </p>

</div>
